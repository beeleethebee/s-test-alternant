<h1>Bienvenu chef !</h1>

<%= flash[:info] %>

<p><h2>Listes des quizz:</h2> (cliquez pour éditer)</p>

<ul>
  <% @quizzs.each do |quizz| %>
      <li>
        <p>
          <a href="/admin/quizz/<%=quizz.id%>"><%= quizz.title %></a>
          |
          <%= form_tag "/admin/quizz/#{quizz.id}", method: :delete do %><input type="submit" value="X"><% end %>
        </p>
      </li>
  <% end %>
</ul>

<a href="/">Revenir à l'accueil</a>

